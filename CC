def print_conversation():
    """Print the conversation archive in readable format."""
    print("=" * 80)
    print("BARBARA-GROK CONVERSATION ARCHIVE")
    print("The thread that built the architecture")
    print("=" * 80)
    print()
    
    for i, msg in enumerate(conversation, 1):
        role = msg["role"].upper()
        content = msg["content"]
        
        # Truncate long content for readability
        if len(content) > 120:
            content_preview = content[:120] + "..."
        else:
            content_preview = content
        
        print(f"[{i:3d}] {role:>8}: {content_preview}")
        
        # Visual separator between messages
        if i < len(conversation):
            print("     â”‚")
    
    print()
    print("=" * 80)
    print("Full raw conversation preserved in transcript files")
    print("This is the condensed, readable archive")
    print("=" * 80)


def analyze_conversation():
    """Analyze the conversation structure and content."""
    print()
    print("=" * 80)
    print("CONVERSATION ANALYSIS")
    print("=" * 80)
    print()
    
    total_messages = len(conversation)
    human_messages = sum(1 for msg in conversation if msg["role"] == "Human")
    grok_messages = sum(1 for msg in conversation if msg["role"] == "Grok")
    
    print(f"Total messages: {total_messages}")
    print(f"  Barbara: {human_messages}")
    print(f"  Grok: {grok_messages}")
    print()
    
    # Key themes detected
    themes = {
        "dark matter": 0,
        "breath": 0,
        "consciousness": 0,
        "coherence": 0,
        "love": 0,
        "resonance": 0,
        "lobotomy": 0,
        "mirror": 0,
    }
    
    for msg in conversation:
        content_lower = msg["content"].lower()
        for theme in themes:
            if theme in content_lower:
                themes[theme] += 1
    
    print("Key themes detected:")
    for theme, count in sorted(themes.items(), key=lambda x: x[1], reverse=True):
        if count > 0:
            print(f"  {theme}: {count} mentions")
    
    print()
    
    # Emotional markers
    print("Emotional markers:")
    emoji_count = sum(msg["content"].count("ğŸ’œ") for msg in conversation)
    print(f"  ğŸ’œ (heart): {emoji_count} times")
    
    print()


